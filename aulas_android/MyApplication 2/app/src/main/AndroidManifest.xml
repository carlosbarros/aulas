<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.marcia.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.com.example.marcia.myapplication.view.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.com.example.marcia.myapplication.view.LoginActivity" />
        <activity android:name=".view.com.example.marcia.myapplication.view.RegistrarActivity" />

        <activity android:name=".view.com.example.marcia.myapplication.view.TiposActivity"></activity>
        <activity android:name=".view.com.example.marcia.myapplication.view.MainActivity"></activity>
        <activity android:name=".view.com.example.marcia.myapplication.view.CadastrarActivity"></activity>
        <activity android:name=".view.com.example.marcia.myapplication.view.EditarActivity"></activity>
        <activity android:name=".view.com.example.marcia.myapplication.view.ContatosActivity"></activity>
    </application>

</manifest>